<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:web="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<servlet>
		<servlet-name>RegisterAction</servlet-name>
		<servlet-class>com.zhaoyan.webserver.register.RegisterAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegisterAction</servlet-name>
		<url-pattern>/servlet/RegisterAction</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>LoginAction</servlet-name>
		<servlet-class>com.zhaoyan.webserver.login.LoginAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginAction</servlet-name>
		<url-pattern>/servlet/LoginAction</url-pattern>
	</servlet-mapping>

</web-app>